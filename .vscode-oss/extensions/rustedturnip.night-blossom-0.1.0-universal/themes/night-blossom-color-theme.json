    /* ----------------------------------------- */
    /* ------------ Semantic Tokens ------------ */
    /* ----------------------------------------- */
  "semanticTokenColors": {
    "variable": "#6ADCEA",
    "keyword": "#eb73bf",
    "typeParameter": "#eb73bf",
    "namespace": "#ffd5bd",
    "type": "#ffb3ff",
    "method": "#A99CFF",
    "function": "#A99CFF",
    "parameter": "#6ADCEA",
    "comment": "#bfa6b6",
    "number": "#ffa14a",
    "operator": "#ff99ff",
  },
  "tokenColors": [

    /* ----------------------------------------- */
    /* ------------ General Colours ------------ */
    /* ----------------------------------------- */
    {
      "name": "General Keywords",
      "scope": [
        "keyword",
        "storage.type.struct",
        "storage.modifier",
        "storage.type.modifier.access",
        "storage.type.class",
        "constant.language"
      ],
      "settings": {
        "foreground": "#eb73bf",
      }
    },
    {
      "name": "String Literals",
      "scope": "string",
      "settings": {
        "foreground": "#ffbab8"
      }
    },
    {
      "name": "Genral Types",
      "scope": [
        "storage.type",
        "entity.name.type",
        "punctuation.definition",
        "support.type"
      ],
      "settings": {
        "foreground": "#ffb3ff"
      }
    },
    {
      "name": "General Variables",
      "scope": [
        "variable",
        "meta.block",
        "meta.function.definition.parameters",
        "meta.body.function.definition",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#6ADCEA"
      }
    },
    {
      "name": "General Functions",
      "scope": [
          "entity.name.function"
      ],
      "settings": {
        "foreground": "#A99CFF"
      }
    },
    {
      "name": "General Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator.scope-resolution",
        "punctuation"
      ],
      "settings": {
        "foreground": "#ff99ff",
      }
    },
    {
      "name": "General Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#bfa6b6",
      }
    },
    {
      "name": "General Namespace",
      "scope": [
        "entity.name.scope-resolution",
        "storage.modifier.import",
        "storage.modifier.package",
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#ffd5bd",
      }
    },
    {
      "name": "General Import Paths",
      "scope": [
        "entity.name.import"
      ],
      "settings": {
        "foreground": "#b8aa98"
      }
    },
    {
      "name": "General Numeric",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#ffa14a"
      }
    },
    {
      "name": "General This",
      "scope": [
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#47a3ff",
      }
    },
    {
      "name": "General Markup Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#eb73bf",
      }
    },
    {
      "name": "General Markup Meta Tags",
      "scope": [
        "meta.tag",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#ffbab8",
      }
    },
    {
      "name": "General Markup Attributes",
      "scope": [
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#6ADCEA",
      }
    },

    /* ------------------------------------ */
    /* ------------ Go Colours ------------ */
    /* ------------------------------------ */
    {
      "name": "Variables",
      "scope": "source.go constant",
      "settings": {
        "foreground": "#6ADCEA"
      }
    },

    /* --------------------------------------- */
    /* ---------- JSON File Colours ---------- */
    /* --------------------------------------- */
    {
      "name": "JSON Keys (Property Names)",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#6ADCEA"
      }
    },
    {
      "name": "JSON Values (string)",
      "scope": "meta.structure.dictionary.value.json string.quoted.double",
      "settings": {
        "foreground": "#ffbab8"
      }
    },
    {
      "name": "JSON Values (numeric)",
      "scope": "meta.structure.dictionary.value constant.numeric.json",
      "settings": {
        "foreground": "#ffa14a"
      }
    },
    {
      "name": "JSON Language Constants",
      "scope": "meta.structure.dictionary.json meta.structure.dictionary.value constant.language",
      "settings": {
        "foreground": "#eb73bf"
      }
    },

    /* --------------------------------------- */
    /* --------- Python File Colours --------- */
    /* --------------------------------------- */
    {
      "name": "Python Keywords",
      "scope": [
        "constant.language.python",
        "meta.function.python",
        "storage.type.function.python",
        "storage.type.class.python"
      ],
      "settings": {
        "foreground": "#eb73bf"
      }
    },
    {
      "name": "Pyhton Types",
      "scope": [
        "meta.class.python",
        "support.type.exception.python",
        "meta.function.parameters.python",
        "support.type.python"
      ],
      "settings": {
        "foreground": "#ffb3ff"
      }
    },
    {
      "name": "Python Variables",
      "scope": [
        "variable.parameter.function.language.python",
        "variable.language.special.self.python",
        "meta.attribute.python",
        "variable.parameter.function-call.python"
      ],
      "settings": {
        "foreground": "#6ADCEA"
      }
    },
    {
      "name": "Python Function Call",
      "scope": [
        "meta.function-call.python",
        "meta.function-call.generic.python",
        "entity.name.type.class.python",
        "entity.name.function.python",
        "meta.function.python",
        "support.function.builtin.python"
      ],
      "settings": {
        "foreground": "#A99CFF"
      }
    },
    {
      "name": "Python Function Decorators",
      "scope": [
        "entity.name.function.decorator.python",
        "meta.function.decorator.python"
      ],
      "settings": {
        "foreground": "#ffd5bdDD"
      }
    },

    /* ----------------------------------------- */
    /* ------------ C# File Colours ------------ */
    /* ----------------------------------------- */
    {
      "name": "C# Meta Tags",
      "scope": [
        "meta.tag.cs"
      ],
      "settings": {
        "foreground": "#eb73bf",
